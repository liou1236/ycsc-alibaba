<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>hello_one</servlet-name>
    <servlet-class>main.com.wjh.servlet.HelloServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>hello_two</servlet-name>
    <servlet-class>main.com.wjh.servlet.HelloServletTwo</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>hello_one</servlet-name>
    <url-pattern>/hello.action</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>hello_two</servlet-name>
    <url-pattern>/abc</url-pattern>
  </servlet-mapping>
</web-app>
